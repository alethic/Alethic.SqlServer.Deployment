<Project>
    <PropertyGroup>
        <Authors>Jerome Haltom</Authors>
        <Company>Alethic Solutions</Company>
        <RepositoryUrl>https://github.com/alethic/Cogito.SqlServer.Deployment</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageProjectUrl>https://github.com/alethic/Cogito.SqlServer.Deployment</PackageProjectUrl>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageIcon>icon.png</PackageIcon>
        <LangVersion>8.0</LangVersion>
        <PackageTags>sql dacpac dacfx dac</PackageTags>
        <RunSettingsFilePath>$(MSBuildThisFileDirectory)\Cogito.SqlServer.Deployment.runsettings</RunSettingsFilePath>
    </PropertyGroup>
    <ItemGroup>
        <None Include="$(MSBuildThisFileDirectory)icon.png" Pack="true" PackagePath="" />
    </ItemGroup>
</Project>
